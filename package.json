{
  "name": "yaml-sort",
  "main": "./lib/yaml-sort",
  "version": "0.6.0",
  "description": "Sort YAML keys and validate YAML (fork from elledienne)",
  "keywords": [
    "yaml",
    "sort",
    "validate",
    "autosort"
  ],
  "activationCommands": {
    "atom-workspace": "yaml-sort:validate"
  },
  "activationHooks": [
    "language-yaml:grammar-used"
  ],
  "repository": "https://github.com/KouroshVivan/yaml-sort",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "js-yaml": ">=4"
  }
}
